<div class="page-header">
  <h1>
    Old jobs
    <a href="/old_jobs/new" class="btn btn-lg btn-success">New Old job</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>User</th>
        <th>Actions</th>
      </tr>

      <% @old_jobs.each do |old_job| %>
      <tr>
        <td><%= old_job.user_id %></td>
        <td>
          <a href="/old_jobs/<%= old_job.id %>" class="btn btn-primary">Show</a>
          <a href="/old_jobs/<%= old_job.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_old_job/<%= old_job.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
