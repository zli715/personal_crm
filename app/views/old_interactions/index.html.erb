<div class="page-header">
  <h1>
    Old interactions
    <a href="/old_interactions/new" class="btn btn-lg btn-success">New Old interaction</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Connect</th>
        <th>Description</th>
        <th>Actions</th>
      </tr>

      <% @old_interactions.each do |old_interaction| %>
      <tr>
        <td><%= old_interaction.connect_id %></td>
        <td><%= old_interaction.description %></td>
        <td>
          <a href="/old_interactions/<%= old_interaction.id %>" class="btn btn-primary">Show</a>
          <a href="/old_interactions/<%= old_interaction.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_old_interaction/<%= old_interaction.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
