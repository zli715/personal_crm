<div class="page-header">
  <h3>Old Interaction #<%= @old_interaction.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Connect</dt>
      <dd><%= @old_interaction.connect_id %></dd>

      <dt>Description</dt>
      <dd><%= @old_interaction.description %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/old_interactions" class="btn btn-primary">
        Back
      </a>
      <a href="/old_interactions/<%= @old_interaction.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_old_interaction/<%= @old_interaction.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
